{
  "manifest_version": 3,
  "name": "Phishing URL Checker",
  "version": "1.0",
  "description": "Detects if a URL is safe or phishing",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Phishing URL Detector"
  }
}
